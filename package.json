      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^9.6.4",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.3",
    "postcss-import": "^12.0.1",
    "postcss-nested": "^4.1.2",
